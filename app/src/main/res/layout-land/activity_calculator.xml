<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="726dp"
        android:layout_height="434dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="1dp"
            android:orientation="vertical">

            <EditText
                android:id="@+id/tvResult"
                android:layout_width="match_parent"
                android:layout_height="67dp"
                android:paddingHorizontal="15dp"
                android:paddingVertical="35dp"
                android:text=" "
                android:textColor="#000000"
                android:textSize="30dp"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/tvSolution"
                android:layout_width="match_parent"
                android:layout_height="66dp"
                android:paddingHorizontal="15dp"
                android:paddingVertical="35dp"
                android:text=""
                android:textColor="#000000"
                android:textSize="30dp"
                android:textStyle="bold" />

            <TableLayout
                android:id="@+id/tableLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp">

                <TableRow
                    android:layout_width="335dp"
                    android:layout_height="57dp"
                    android:layout_gravity="center">

                    <Button
                        android:id="@+id/calcAC"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#AF69EE"
                        android:text="AC"
                        android:textColor="@color/white"
                        android:textSize="15dp"
                        app:cornerRadius="36dp" />

                    <Button
                        android:id="@+id/calcC"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#AF69EE"
                        android:text="C"
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                    <Button
                        android:id="@+id/calcOpen"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#AF69EE"
                        android:text="("
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                    <Button
                        android:id="@+id/calcClose"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#AF69EE"
                        android:text=")"
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                    <Button
                        android:id="@+id/calcDiv"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#FFA500"
                        android:text="/"
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                    <Button
                        android:id="@+id/calcMult"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#FFA500"
                        android:text="*"
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                    <Button
                        android:id="@+id/calcMin"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#FFA500"
                        android:text="-"
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <Button
                        android:id="@+id/calcFive"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#0096C7"
                        android:text="5"
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                    <Button
                        android:id="@+id/calcSix"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#0096C7"
                        android:text="6"
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                    <Button
                        android:id="@+id/calcSeven"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#0096C7"
                        android:text="7"
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                    <Button
                        android:id="@+id/calcEight"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#0096C7"
                        android:text="8"
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                    <Button
                        android:id="@+id/calcNine"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#0096C7"
                        android:text="9"
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                    <Button
                        android:id="@+id/calcAdd"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#FFA500"
                        android:text="+"
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <Button
                        android:id="@+id/calcZero"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#0096C7"
                        android:text="0"
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                    <Button
                        android:id="@+id/calcOne"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#0096C7"
                        android:text="1"
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                    <Button
                        android:id="@+id/calcTwo"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#0096C7"
                        android:text="2"
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                    <Button
                        android:id="@+id/calcThree"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#0096C7"
                        android:text="3"
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                    <Button
                        android:id="@+id/calcFour"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#0096C7"
                        android:text="4"
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                    <Button
                        android:id="@+id/calcDecimal"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#FFA500"
                        android:text="."
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                    <Button
                        android:id="@+id/calcEquals"
                        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_margin="12dp"
                        android:backgroundTint="#FFA500"
                        android:text="="
                        android:textColor="@color/white"
                        android:textSize="32dp"
                        app:cornerRadius="36dp" />

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                </TableRow>
            </TableLayout>

        </LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>